{
  "type":"Check",
  "spec":{
    "name":"nagios_check_ssh_server_localhost",
    "command": "/usr/lib64/nagios/plugins/check_ssh -4 -r {{ .ssh.version | default \"OpenSSH_7.4\" }} -P {{ .ssh.protocol | default \"2.0\" }} localhost",
    "type":"metric",
    "handlers":[
      "logevent"
    ],
    "interval":10,
    "timeout":15,
    "subscriptions":[
      "localhost"
    ],
    "publish":true,
    "organization":"default",
    "environment":"default"
  }
}
